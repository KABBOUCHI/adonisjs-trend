{
  "name": "adonisjs-trend",
  "version": "0.0.6",
  "description": "Generate trends for your models. Easily generate charts or reports.",
  "repository": "KABBOUCHI/adonisjs-trend",
  "author": "Georges KABBOUCHI <georges.kabbouchi@gmail.com>",
  "keywords": [
    "adonisjs",
    "trend",
    "chart",
    "stats",
    "aggregate"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "eslint . --fix && prettier -w .",
    "prepack": "pnpm build"
  },
  "devDependencies": {
    "@adonisjs/tsconfig": "^1.3.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.1.0",
    "eslint": "^9.8.0",
    "eslint-config-unjs": "^0.3.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7"
  },
  "peerDependencies": {
    "@adonisjs/lucid": "^21.3.0",
    "luxon": "^3.4.4"
  },
  "packageManager": "pnpm@9.7.0"
